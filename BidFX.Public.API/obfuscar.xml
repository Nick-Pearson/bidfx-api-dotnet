<?xml version='1.0'?>
<Obfuscator>
    <Var name="InPath" value="." />
    <Var name="OutPath" value="../Obfuscator_Output" />
    <Var name="KeepPublicApi" value="true" />
    <Var name="HidePrivateApi" value="true" />

    <Module file="$(InPath)\BidFX.Public.API.dll">
        <!--  Allow the Excel API to change the product name -->
        <SkipType name="BidFX.Public.API.LoginService" />
        <SkipProperty type="BidFX.Public.API.LoginService" name="Product" />
        <SkipProperty type="BidFX.Public.API.Client" name="LoginService" />
    </Module>
    
</Obfuscator>